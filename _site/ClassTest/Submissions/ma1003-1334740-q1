def recX(n):
    if n == 0: # start at 0#
        return 1
    a=1
    b=1
    i=0
    while i < n: 
        a, b=b, a+b # add pervious two numbers to get next number#
        i += 1
    return a
for i in range(11): # first 11 fibonacci numbers#
      print recX(i)


