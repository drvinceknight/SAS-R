def fibonacci(n):
    """
    A function to calculate the fibonacci sequence.

    Arguments:
        for this particular case n >= 0 and also n < 11

    Outputs:  Fn = Fn-1 + Fn-2
    """
    if n == 0 or n == 1:
        return 1
    a = 1
    b = 1
    i = 0
    while i < n:
        a, b = b, a + b
        i += 1
    return a

for i in range(11): 
    print fibonacci(i)

    
